{
	"name": "medusa-sync",
	"description": "Please enter a description for your extension",
	"icon": "extension",
	"version": "1.0.0",
	"keywords": [
		"directus",
		"directus-extension",
		"directus-extension-hook"
	],
	"type": "module",
	"files": [
		"dist"
	],
	"directus:extension": {
		"type": "hook",
		"path": "dist/index.js",
		"source": "src/index.ts",
		"host": "^10.10.0"
	},
	"scripts": {
		"build": "directus-extension build",
		"dev": "directus-extension build -w --no-minify",
		"link": "directus-extension link"
	},
	"devDependencies": {
		"@directus/extensions-sdk": "12.1.4",
		"@types/node": "^22.10.6",
		"typescript": "^5.7.3"
	},
	"dependencies": {
		"@medusajs/js-sdk": "^2.2.0",
		"@medusajs/types": "^2.2.0"
	}
}
